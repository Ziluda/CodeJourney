# MY SUCCESSFULLY ATTEMPTED CODE

def prime_checker(number):
    if (number != 2 and number != 3) and (number != 5 and number != 7):
        if (number % 2 == 0 or number % 3 == 0) or (number % 5 == 0 or number % 7 == 0):
            print("It's not a prime number.")
        else:
            print("It's a prime number.")    
    else:
        print("It's a prime number.")    

#Write your code above this line ðŸ‘†
    
#Do NOT change any of the code belowðŸ‘‡
n = int(input("Check this number: "))
prime_checker(number=n)


# MODEL CODE

def prime_checker(number):
  is_prime = True
  for n in range(2, number): # you don't need to add a minus one cause the range function doesn't include the upper limit anyway
    if number % n == 0:
      is_prime == False
  if is_prime:
    print("It's a prime number.")
  else:
    print("It's not a prime number.")
  

#Write your code above this line ðŸ‘†
    
#Do NOT change any of the code belowðŸ‘‡
n = int(input("Check this number: "))
prime_checker(number=n)


# LEARN HOW TO FIX THIS SO IT WORKS CORRECTLY


def prime_checker(number):
    if (number != 2 and number != 3) and (number != 5 and number != 7):
        if (number%2 != 0 or number%3 != 0) or (number%5 != 0 or number%7 != 0):
            print("It's a prime number.")
        elif (number%2 != 0 and number%3 != 0) and (number%5 != 0 and number%7 != 0):
            print("It's a prime number.")
    else:
        print("It's not a prime number.")

# EVEN THIS

def prime_checker(number):
    if ((number % 2 != 0) and (number % 3 != 0)) and ((number % 5 != 0) and (number % 7 != 0)):
        print("It's a prime number.")
    else:
        print("It's not a prime number.")

